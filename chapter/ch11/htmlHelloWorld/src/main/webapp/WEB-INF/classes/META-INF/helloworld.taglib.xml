<?xml version='1.0' encoding='UTF-8'?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
   http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
   version="2.0">
  <namespace>http://jsfcompref.com/example</namespace>
  <tag>
    <tag-name>helloworld</tag-name>
    <component><component-type>HtmlHelloWorld</component-type></component>
  </tag>
  <tag>
    <tag-name>helloinput</tag-name>
    <component><component-type>HtmlHelloInput</component-type></component>
  </tag>
  <tag>
    <tag-name>hellorenderer</tag-name>
    <component>
        <component-type>javax.faces.Input</component-type>
        <renderer-type>HtmlHelloWorld</renderer-type>
    </component>
  </tag>
</facelet-taglib>

