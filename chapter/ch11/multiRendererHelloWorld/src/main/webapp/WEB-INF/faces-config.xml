<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

<lifecycle>
  <phase-listener>com.jsfcompref.components.renderer.applemobile.RenderKitSelectorPhaseListener</phase-listener>
</lifecycle>

<render-kit>
  <render-kit-id>HTML_APPLEMOBILE</render-kit-id>
  <render-kit-class>com.jsfcompref.components.renderer.applemobile.AppleMobileRenderKit</render-kit-class>
  <renderer>
    <component-family>javax.faces.Input</component-family>
    <renderer-type>HtmlHelloWorld</renderer-type>
    <renderer-class>com.jsfcompref.components.renderer.HtmlHelloWorldRenderer</renderer-class>
  </renderer>
</render-kit>

</faces-config>
